{
    "serviceName": "kong-aws-masker-elasticache-service",
    "cluster": "kong-elasticache-cluster",
    "taskDefinition": "kong-aws-masker-elasticache",
    "launchType": "FARGATE",
    "platformVersion": "LATEST",
    "desiredCount": 2,
    "enableExecuteCommand": true,
    "networkConfiguration": {
        "awsvpcConfiguration": {
            "subnets": [
                "subnet-dummy-localstack-1",
                "subnet-dummy-localstack-2"
            ],
            "securityGroups": [
                "sg-kong-fargate-security-group"
            ],
            "assignPublicIp": "ENABLED"
        }
    },
    "loadBalancers": [
        {
            "targetGroupArn": "arn:aws:elasticloadbalancing:us-east-1:000000000000:targetgroup/kong-proxy-tg/dummy",
            "containerName": "kong-gateway",
            "containerPort": 8000
        }
    ],
    "serviceRegistries": [
        {
            "registryArn": "arn:aws:servicediscovery:us-east-1:000000000000:service/srv-kong-discovery",
            "containerName": "kong-gateway",
            "containerPort": 8000
        }
    ],
    "deploymentConfiguration": {
        "maximumPercent": 200,
        "minimumHealthyPercent": 50,
        "deploymentCircuitBreaker": {
            "enable": true,
            "rollback": true
        },
        "alarms": {
            "alarmNames": [
                "kong-high-error-rate",
                "kong-high-latency"
            ],
            "enable": true,
            "rollback": true
        }
    },
    "capacityProviderStrategy": [
        {
            "capacityProvider": "FARGATE",
            "weight": 80,
            "base": 1
        },
        {
            "capacityProvider": "FARGATE_SPOT",
            "weight": 20
        }
    ],
    "enableLogging": true,
    "propagateTags": "SERVICE",
    "tags": [
        {
            "key": "Environment",
            "value": "Development"
        },
        {
            "key": "Service",
            "value": "Kong-AWS-Masker"
        },
        {
            "key": "LaunchType",
            "value": "Fargate"
        },
        {
            "key": "ElastiCache",
            "value": "Enabled"
        }
    ]
}