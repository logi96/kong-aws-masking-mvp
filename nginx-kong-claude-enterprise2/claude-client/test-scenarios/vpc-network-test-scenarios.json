{
  "name": "VPC and Network Resource Test Scenarios",
  "description": "Test cases for VPC, subnet, security group masking",
  "scenarios": [
    {
      "id": "vpc-001",
      "name": "VPC Configuration",
      "input": {
        "messages": [
          {
            "role": "user",
            "content": "VPC vpc-12345678 has CIDR 10.0.0.0/16 with subnets subnet-11111111111111111 (10.0.1.0/24) and subnet-22222222222222222 (10.0.2.0/24)"
          }
        ]
      },
      "expected_masking": {
        "patterns": [
          "vpc-12345678",
          "subnet-11111111111111111",
          "subnet-22222222222222222"
        ],
        "preserve_cidr": true
      }
    },
    {
      "id": "vpc-002",
      "name": "Security Group Rules",
      "input": {
        "messages": [
          {
            "role": "user",
            "content": "Security group sg-abcdef12 allows inbound traffic from sg-12345678 on port 443, and sg-87654321 allows SSH from 0.0.0.0/0"
          }
        ]
      },
      "expected_masking": {
        "patterns": [
          "sg-abcdef12",
          "sg-12345678",
          "sg-87654321"
        ]
      }
    },
    {
      "id": "vpc-003",
      "name": "Network ACL Configuration",
      "input": {
        "messages": [
          {
            "role": "user",
            "content": "Network ACL acl-12345678 is associated with subnet-aaabbbcccdddeeeff and allows traffic from vpc-87654321"
          }
        ]
      },
      "validation": {
        "mask_network_resources": true
      }
    },
    {
      "id": "vpc-004",
      "name": "VPC Peering Connection",
      "input": {
        "messages": [
          {
            "role": "user",
            "content": "VPC peering connection pcx-12345678 connects vpc-aaaabbbb (account 123456789012) with vpc-ccccdddd (account 210987654321)"
          }
        ]
      },
      "expected_masking": {
        "patterns": [
          "vpc-aaaabbbb",
          "vpc-ccccdddd",
          "123456789012",
          "210987654321"
        ]
      }
    },
    {
      "id": "vpc-005",
      "name": "Complex Network Topology",
      "input": {
        "messages": [
          {
            "role": "user",
            "content": "Network topology:\n- VPC: vpc-production (10.0.0.0/16)\n  - Public Subnet: subnet-public1a (10.0.1.0/24) with NAT Gateway nat-12345678901234567\n  - Private Subnet: subnet-private1a (10.0.10.0/24)\n  - Security Groups: sg-web-tier, sg-app-tier, sg-db-tier\n  - Route Table: rtb-12345678"
          }
        ]
      },
      "validation": {
        "preserve_network_hierarchy": true,
        "mask_all_identifiers": true
      }
    }
  ]
}